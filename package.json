{
  "name": "juliaspies",
  "version": "0.1.0",
  "description": "Julia's Pies",
  "scripts": {
    "watch:js": "watchify src/index.js --debug --transform babelify --outfile public/bundle.js",
    "watch:css": "sass --watch src/stylesheets:public/",
    "watch": "parallelshell 'npm run watch:js' 'npm run watch:css'",
    "start": "parallelshell 'npm run watch' 'python -m SimpleHTTPServer'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juliahw/juliaspies.git"
  },
  "author": "Julia Wang",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  }
}
